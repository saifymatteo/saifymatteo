  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dev",
      "cwd": "flutter_web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--web-port=8080"
      ]
    },
    {
      "name": "Profile",
      "cwd": "flutter_web",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart",
      "args": [
        "--web-port=8080"
      ]
    },
    {
      "name": "Release",
      "cwd": "flutter_web",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
      "args": [
        "--web-port=8080"
      ]
    }
  ]
}
